  },
  "homepage": "https://github.com/zorkow/AMS-workshop#readme",
  "dependencies": {
    "diagcess": "^1.1.4",
    "http-server": "^14.1.0",
    "markdown-it": "^12.3.2",
    "markdown-it-attrs": "^4.1.3",
    "markdown-it-container": "^3.0.0",
    "markdown-it-header-sections": "^1.0.0",
    "minify": "^5.2.0",
    "normalize.css": "^8.0.1"
  }
